        <Button fx:id="generateReportButton" layoutX="180.0" layoutY="74.0" mnemonicParsing="false" onAction="#generateReportClicked" text="Generate Report" />
        <Button fx:id="goBackButton" layoutX="202.0" layoutY="117.0" mnemonicParsing="false" onAction="#goBack" text="Go Back" />
        <Label fx:id="patientNameLabel" layoutX="30.0" layoutY="100.0" />
        <Label fx:id="patientAgeLabel" layoutX="30.0" layoutY="130.0" />
        <Label fx:id="diagnosisLabel" layoutX="30.0" layoutY="160.0" />
        <Label fx:id="treatmentLabel" layoutX="30.0" layoutY="190.0" />
    </children>
</AnchorPane>
